{
  "name": "@fdmg/ts-react-player-controls",
  "version": "1.0.24",
  "description": "ReactJS PlayerControls component",
  "main": "dist/PlayerControls.js",
  "types": "dist/PlayerControls.d.ts",
  "scripts": {
    "release": "npm update && npm install && yarn && npm run test && npm run lint && npm run sass && npm run tsc",
    "sass": "node-sass --output-style=compressed scss/PlayerControls.scss > dist/PlayerControls.css",
    "tsc": "tsc -d",
    "test": "jest",
    "lint": "tslint -p .",
    "updateSnapshot": "jest --updateSnapshot",
    "u": "npm test -- -u",
    "webpack-demo": "webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:FDMediagroep/fdmg-ts-react-player-controls.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Willem Liu, Alexander Eerenberg",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/react": "^16.3.13",
    "awesome-typescript-loader": "^5.0.0",
    "coveralls": "^3.0.0",
    "css-loader": "^2.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.4.0",
    "enzyme-to-json": "^3.3.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.0",
    "jest": "^22.4.3",
    "jest-cli": "^23.0.0",
    "jest-localstorage-mock": "^2.2.0",
    "node-sass": "^4.9.0",
    "preact": "^8.2.7",
    "preact-compat": "^3.18.0",
    "raf": "^3.4.0",
    "react": "16.8.1",
    "react-dom": "^16.8.1",
    "react-test-renderer": "^16.8.1",
    "sass-loader": "^7.0.0",
    "style-loader": "^0.23.0",
    "ts-jest": "23.10.5",
    "tslint": "^5.9.1",
    "tslint-react": "^3.5.1",
    "typescript": "^2.8.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^3.0.3"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "setupFiles": [
      "raf/polyfill",
      "jest-localstorage-mock"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "collectCoverage": true,
    "coverageDirectory": "dist/coverage",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "dependencies": {}
}
