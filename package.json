{
  "name": "@fdmg/ts-react-player-controls",
  "version": "1.0.10",
  "description": "ReactJS PlayerControls component",
  "main": "dist/PlayerControls.js",
  "types": "dist/PlayerControls.d.ts",
  "scripts": {
    "release": "npm update && npm install && yarn && npm run test && npm run lint && npm run sass && npm run tsc",
    "sass": "node-sass --output-style=compressed scss/PlayerControls.scss > dist/PlayerControls.css",
    "tsc": "tsc -d",
    "test": "jest",
    "lint": "tslint -p .",
    "updateSnapshot": "jest --updateSnapshot",
    "u": "npm test -- -u",
    "webpack-demo": "set NODE_ENV=production&& webpack --env production"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:FDMediagroep/fdmg-ts-react-player-controls.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Willem Liu, Alexander Eerenberg",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/react": "^16.0.31",
    "awesome-typescript-loader": "^3.4.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "jest": "^22.0.1",
    "jest-cli": "^22.0.1",
    "node-sass": "^4.7.2",
    "preact": "^8.2.7",
    "preact-compat": "^3.17.0",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "ts-jest": "^22.0.0",
    "tslint": "^5.8.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "setupFiles": [
      "raf/polyfill"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "collectCoverage": true,
    "coverageDirectory": "dist/coverage",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ],
    "mapCoverage": true
  },
  "dependencies": {
    "@fdmg/ts-react-image-button": "^1.0.24"
  }
}
